<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/iVCarEditPage"
        android:layout_width="match_parent"
        android:layout_height="245dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_help" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="223dp"
        android:scaleType="fitXY"
        android:src="@drawable/rectangle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnLogin3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="200dp"
        android:width="180dp"
        android:backgroundTint="#F3F3F3"
        android:fontFamily="@font/poppins"
        android:text="Delete car"
        android:textColor="#2E2E2E"
        android:textSize="20dp"
        app:cornerRadius="200dp"
        app:layout_constraintBottom_toBottomOf="@+id/btnRegister3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:strokeColor="#CC0B6FF4"
        app:strokeWidth="1dp" />

    <Button
        android:id="@+id/btnRegister3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="200dp"
        android:layout_marginBottom="16dp"
        android:width="180dp"
        android:backgroundTint="#0B6FF4"
        android:fontFamily="@font/poppins"
        android:text="See incomes"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        app:cornerRadius="200dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />

    <TextView
        android:id="@+id/tvYearCarEditPage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/poppins"
        android:text="2012"
        android:textColor="#2F2F2F"
        android:textSize="18dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iVCarEditPage" />

    <TextView
        android:id="@+id/tvTitleCarEditPage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-8dp"
        android:fontFamily="@font/poppins"
        android:text="Hyundai i10"
        android:textColor="#2F2F2F"
        android:textSize="32dp"
        app:layout_constraintStart_toStartOf="@+id/tvYearCarEditPage"
        app:layout_constraintTop_toBottomOf="@+id/tvYearCarEditPage" />

    <ImageView
        android:id="@+id/imageView222"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:scaleType="fitXY"
        android:src="@drawable/recentlage_car_info"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvTitleCarEditPage" />


    <Button
        android:id="@+id/btnEditCarProperties"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="80dp"
        android:height="20dp"
        android:backgroundTint="#F3F3F3"
        android:fontFamily="@font/poppins"
        android:scaleX="0.5"

        android:scaleY="0.5"
        android:text="Edit"
        android:textColor="#2E2E2E"
        android:textSize="20dp"
        app:cornerRadius="200dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView222"
        app:layout_constraintEnd_toEndOf="@+id/imageView222"
        app:strokeColor="#CC0B6FF4"
        app:strokeWidth="1dp" />

    <ImageView
        android:id="@+id/imageView2223"
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:scaleType="fitXY"
        android:src="@drawable/recentlage_car_info"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView222" />

    <Button
        android:id="@+id/btnEditCarPrice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="80dp"
        android:height="20dp"
        android:backgroundTint="#F3F3F3"
        android:fontFamily="@font/poppins"

        android:scaleX="0.5"
        android:scaleY="0.5"
        android:text="Edit"
        android:textColor="#2E2E2E"
        android:textSize="20dp"
        app:cornerRadius="200dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2223"
        app:layout_constraintEnd_toEndOf="@+id/imageView2223"
        app:strokeColor="#CC0B6FF4"
        app:strokeWidth="1dp" />

    <EditText
        android:id="@+id/tvCarEditPageDailyPrice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-8dp"
        android:backgroundTint="@android:color/transparent"
        android:editable="false"
        android:fontFamily="@font/poppins"
        android:text="10$"
        android:textColor="#2F2F2F"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="@+id/tvCarEditPageHourlyPrice"
        app:layout_constraintTop_toTopOf="@+id/textView11" />

    <EditText
        android:id="@+id/tvCarEditPageHourlyPrice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-8dp"
        android:backgroundTint="@android:color/transparent"
        android:editable="false"
        android:fontFamily="@font/poppins"
        android:text="10$"
        android:textColor="#2F2F2F"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="@+id/tvPowerCarEditPage"
        app:layout_constraintTop_toTopOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/poppins"
        android:text="Price for day"
        android:textColor="#2F2F2F"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/poppins"
        android:text="Price for hour"
        android:textColor="#2F2F2F"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="@+id/imageView2223"
        app:layout_constraintTop_toTopOf="@+id/imageView2223" />

    <EditText
        android:id="@+id/tvPowerCarEditPage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins"
        android:text="Places"
        android:textColor="#2F2F2F"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView10"
        app:layout_constraintStart_toStartOf="@+id/tvAcCarEditPage"
        app:layout_constraintTop_toTopOf="@+id/textView10"
        android:editable="false"
        android:backgroundTint="@android:color/transparent"/>

    <EditText
        android:id="@+id/tvAcCarEditPage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins"
        android:text="Places"
        android:textColor="#2F2F2F"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="@+id/tvGearboxCarEditPage"
        app:layout_constraintTop_toTopOf="@+id/textView9"
        android:editable="false"
        android:backgroundTint="@android:color/transparent"/>

    <EditText
        android:id="@+id/tvGearboxCarEditPage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins"
        android:text="Places"
        android:textColor="#2F2F2F"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView8"
        app:layout_constraintStart_toStartOf="@+id/tvDoorsCarEditPage"
        app:layout_constraintTop_toTopOf="@+id/textView8"
        android:editable="false"
        android:backgroundTint="@android:color/transparent"/>

    <EditText
        android:id="@+id/tvDoorsCarEditPage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins"
        android:text="Places"
        android:textColor="#2F2F2F"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintStart_toStartOf="@+id/tvPlacesCarEditPage"
        app:layout_constraintTop_toTopOf="@+id/textView7"
        android:editable="false"
        android:backgroundTint="@android:color/transparent"/>

    <EditText
        android:id="@+id/tvPlacesCarEditPage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="160dp"
        android:fontFamily="@font/poppins"
        android:text="Places"
        android:textColor="#2F2F2F"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView6"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="@+id/textView6"
        android:editable="false"
        android:backgroundTint="@android:color/transparent"/>

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/poppins"
        android:text="Power"
        android:textColor="#2F2F2F"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/poppins"
        android:text="Air Conditioner"
        android:textColor="#2F2F2F"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/poppins"
        android:text="Gearbox"
        android:textColor="#2F2F2F"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/poppins"
        android:text="Doors"
        android:textColor="#2F2F2F"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/poppins"
        android:text="Places"
        android:textColor="#2F2F2F"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="@+id/imageView222"
        app:layout_constraintTop_toTopOf="@+id/imageView222" />


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleX="0.9"
        android:scaleY="0.8"
        android:src="@drawable/arrow_back"
        app:layout_constraintStart_toStartOf="@+id/iVCarEditPage"
        app:layout_constraintTop_toTopOf="@+id/iVCarEditPage"
        android:onClick="backToMainPage"></ImageView>


</androidx.constraintlayout.widget.ConstraintLayout>